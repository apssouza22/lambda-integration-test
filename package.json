{
  "name": "cdk-ts-stack",
  "version": "0.1.0",
  "bin": {
    "cdk-ts-stack": "bin/cdk-ts-stack.js"
  },
  "scripts": {
    "build": "tsc -b .",
    "watch": "tsc -b -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "tsc -b --clean",
    "deploy": "cdk deploy",
    "synth": "cdk synth",
    "pretty": "prettier --ignore-unknown --write .",
    "test:watch": "jest --watchAll",
    "lint:eslint": "eslint --ext ts .",
    "lint:eslint:fix": "eslint --ext ts --fix .",
    "lint": "run-s lint:eslint",
    "lintfix": "run-s lintfix:eslint"
  },
  "prettier": {
    "semi": false,
    "useTabs": true,
    "printWidth": 100,
    "singleQuote": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "trailingComma": "none"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.108",
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "aws-cdk": "2.50.0",
    "esbuild": "^0.16.10",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-cdk": "^1.0.2",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "jest-junit": "^14.0.1",
    "jest-junit-reporter": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.180.0",
    "@aws-cdk/aws-apigatewayv2-alpha": "2.50.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers": "1.180.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "2.50.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "1.180.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.50.0-alpha.0",
    "@aws-cdk/aws-lambda": "1.180.0",
    "@aws-cdk/aws-lambda-nodejs": "1.180.0",
    "@aws-cdk/aws-lambda-python": "1.180.0",
    "@aws-cdk/aws-lambda-python-alpha": "2.50.0-alpha.0",
    "@aws-cdk/core": "1.180.0",
    "aws-cdk-lib": "2.50.0",
    "aws-lambda": "1.0.7",
    "constructs": "10.0.0",
    "source-map-support": "^0.5.21",
    "express": "^4.17.1"
  }
}
